{
  "name": "vue-cesium-vite-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build-with-check": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "clean": "bunx rimraf ./dist",
    "deploy-mah": "vite build --base=/apps/$npm_package_name/ && rsync -avz --delete dist/ mah@mah.priv.at:/var/www/static.mah.priv.at/apps/$npm_package_name/ && echo visit https://static.mah.priv.at/apps/$npm_package_name"
  },
  "dependencies": {
    "echarts": "^5.6.0",
    "vite-plugin-devtools-json": "^0.3.0",
    "vite-plugin-qrcode": "^0.3.0",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue": "^3.5.18",
    "vue-cesium": "^3.2.9"
  },
  "devDependencies": {
    "@babel/types": "^7.28.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/compiler-sfc": "^3.5.18",
    "eslint": "^9.32.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.3.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vue-tsc": "^3.0.4"
  }
}
